<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin's Profile"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewNote"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toEndOf="@+id/ImageViewProfileAdmin"
        app:layout_constraintTop_toBottomOf="@+id/textViewWelcomeBackAdmin"
        app:layout_constraintVertical_bias="0.043" />

    <TextView
        android:id="@+id/tvUsernameAdminProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="116dp"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toTopOf="@+id/textViewNote"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.289"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMyInfo"
        app:layout_constraintVertical_bias="0.181" />

    <TextView
        android:id="@+id/tvEmailAdminProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="76dp"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toTopOf="@+id/textViewNote"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.289"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvUsernameProfile"
        app:layout_constraintVertical_bias="0.244" />

    <TextView
        android:id="@+id/tvWorkingIdAdminProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="36dp"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toTopOf="@+id/textViewNote"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.289"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvEmailProfile" />

    <TextView
        android:id="@+id/tvMyInfoAdmin"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:fontFamily="@font/carter_one"
        android:gravity="center"
        android:text="My Info:"
        android:textColor="#ffffff"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.348" />

    <TextView
        android:id="@+id/textViewWelcomeBackAdmin"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:fontFamily="@font/carter_one"
        android:gravity="center"
        android:text="Welcome Back!"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.924"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119" />

    <TextView
        android:id="@+id/textViewNote"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:fontFamily="@font/carter_one"
        android:gravity="center"
        android:text="Please connect to the company's Wi-Fi before doing this"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheckInAdmin"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:background="@drawable/button_bg_round"
        android:fontFamily="@font/carter_one"
        android:gravity="center"
        android:text="Check-In"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.949" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCheckOutAdmin"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:background="@drawable/button_bg_round"
        android:fontFamily="@font/carter_one"
        android:gravity="center"
        android:text="Check-Out"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.729"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.947" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/ImageViewProfileAdmin"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/round_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.202"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072"
        app:layout_constraintWidth_percent="0.5" />


</androidx.constraintlayout.widget.ConstraintLayout>